{% block supply %}
	<section class="supply">
		<div class="container">
			<div class="supply__wrap">
				<div class="supply__heading">
					<h2>Новые поступления</h2>
					<div class="supply__heading-switch">
						<button class="supply__heading-switch-btn">
							{% include '/assets/svg/chevron-left.svg' %}
						</button>
						<button class="supply__heading-switch-btn">
							{% include '/assets/svg/chevron-right.svg' %}
						</button>
					</div>
				</div>
				<div class="supply__catalog">
					<div class="supply__catalog-card">
						<div class="supply__catalog-offer">
							<p>Бестселлер</p>
							<p>1+1+3</p>
						</div>
						<img src="img/north-south.png" alt="north-south">
						<div class="supply__catalog-card-desc">
							<p class="">Корпоративное право</p>
							<p class="">Инвестирование: способы, риски, субъекты : монография</p>
							<p class="">Майфат А.В.</p>
							<p class="">4 450 ₽</p>
						</div>
						<div class="supply__catalog-card-rating">
							<p>
								{% include '/assets/svg/star-1.svg' %} 4.9
							</p>
							<p> {% include '/assets/svg/like.svg' %} 89% рекомендуют</p>
						</div>
						<div class="supply__catalog-actions">
							<button class="g-button-cart">
								{% include '/assets/svg/orange-cart.svg' %}
								В корзину
							</button>
							<button class="g-button-tabs">
								{% include '/assets/svg/tabs.svg' %}
							</button>
						</div>
					</div>
					<div class="supply__catalog-card">
					
					</div>
					<div class="supply__catalog-card">
					
					</div>
					<div class="supply__catalog-card">
					
					</div>
				</div>
				<div class="supply__show">
					<p>Показать все книги раздела</p>
				</div>
			</div>
		</div>
	</section>
{% endblock %}